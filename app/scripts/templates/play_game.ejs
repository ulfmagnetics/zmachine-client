<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><%= game.description() %> (PID <%= game.get('pid') %>)</h3>
  </div>
  <div class="panel-body">
    <pre class="gametext"><%= game.actions().currentStatus() %></pre>
    <form>
      &rarr; <input type="text" name="action" class='action' />
      <button class="submit btn btn-lg btn-success" href="#">Go</button>
    </form>
  </div>
</div>
